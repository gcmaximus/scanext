{
	"manifest_version": 2,
	"name": "<h1> replacer",
	"version": "1.0",
	"description": "A simple extension that accepts user input",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png",
		"default_title": "My Extension"
	},
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}
